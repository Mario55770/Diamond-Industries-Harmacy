<Defs>
  <!--New research tab-->
  <ResearchTabDef>
    <defName>DIHarmacy</defName>
    <label>Diamond Industry Harmacy</label>
  </ResearchTabDef>
  
  <!--effectively a prerequisite research. Unlocks the main bench and the rest of the research tree.-->
