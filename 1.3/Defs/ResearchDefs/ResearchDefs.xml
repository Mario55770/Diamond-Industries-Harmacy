    <!--electricity+drug production itself-->
    <baseCost>2000</baseCost>
    <techLevel>Industrial</techLevel>
    <prerequisites>
      <li>Electricity</li>
      <li>DrugProduction</li>
    </prerequisites>
    <researchViewX>0.00</researchViewX>
    <researchViewY>0.00</researchViewY>
  </ResearchProjectDef>
